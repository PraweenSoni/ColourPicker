{
    "manifest_version": 2,
    "name": "Color Picker",
    "version": "1.0",
    "description": "Pick Color",
    "permissions": ["activeTab", "tabs"],
    "browser_action": {
      "default_popup": "index.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["main.js"]
      }
    ]
  }